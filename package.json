    "nuqs": "2.2.1",
    "oslo": "1.2.1",
    "pino": "9.5.0",
    "pino-pretty": "13.0.0",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-custom-scrollbars-2": "4.5.0",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-error-boundary": "4.1.2",
    "react-hook-form": "7.53.2",
    "react-i18next": "15.1.1",
    "react-icons": "5.3.0",
    "react-modal-sheet": "3.3.0",
