    "@react-email/render": "1.0.2",
    "@sendgrid/mail": "8.1.4",
    "@t3-oss/env-nextjs": "0.11.1",
    "@tanstack/react-query": "5.61.0",
    "@tanstack/react-query-devtools": "5.61.0",
    "@trpc/client": "11.0.0-rc.638",
    "@trpc/react-query": "11.0.0-rc.638",
    "@trpc/server": "11.0.0-rc.638",
    "bcryptjs": "^3.0.2",
